{
  "name": "ng-radio",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint:fix": "ng lint --fix ng-radio",
    "test-single-headless": "ng test --watch=false --code-coverage --browsers=ChromeHeadless --reporters=progress,junit",
    "test-chrome": "ng test --browsers=Chrome",
    "test-firefox": "ng test --browsers=Firefox",
    "test:coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "test:generate-publish-coverage": "npm run test-single-headless && npm run test:coveralls",
    "analyze-bundle": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/ng-radio/stats.json",
    "format:check": "prettier --list-different \"src/**/*{.ts,.json,.scss}\"",
    "format:fix": "prettier --write \"src/**/*{.ts,.json,.scss}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.10",
    "@angular/cdk": "^17.1.2",
    "@angular/common": "^18.2.10",
    "@angular/compiler": "^18.2.10",
    "@angular/core": "^18.2.10",
    "@angular/forms": "^18.2.10",
    "@angular/material": "^17.1.2",
    "@angular/platform-browser": "^18.2.10",
    "@angular/platform-browser-dynamic": "^18.2.10",
    "@angular/router": "^18.2.10",
    "@microsoft/applicationinsights-web": "^2.7.4",
    "@ngrx/effects": "^18.1.1",
    "@ngrx/router-store": "^18.1.1",
    "@ngrx/store": "^18.1.1",
    "@ngrx/store-devtools": "^18.1.1",
    "core-js": "^3.21.1",
    "dayjs": "1.10.8",
    "is-falsy-or-whitespace": "0.0.17",
    "lodash-es": "^4.17.21",
    "material-design-icons": "^3.0.1",
    "ngx-mat-select-search": "^7.0.5",
    "oidc-client": "^1.11.5",
    "primeicons": "^5.0.0",
    "primeng": "^17.5.0",
    "rxjs": "^7.5.0",
    "socket.io-client": "^4.7.5",
    "subsink": "^1.0.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.14.10",
    "@ngrx/operators": "^18.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.11",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "^17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^18.2.11",
    "@angular/compiler-cli": "^18.2.10",
    "@angular/language-service": "^18.2.10",
    "@ngrx/schematics": "^18.1.1",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jasmine-core": "~4.0.1",
    "jasmine-marbles": "^0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.5.1",
    "protractor": "~7.0.0",
    "puppeteer": "^21.11.0",
    "ts-node": "~10.7.0",
    "typescript": "5.5.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}