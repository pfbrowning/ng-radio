{
  "name": "ng-radio",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint:fix": "ng lint --fix",
    "test-single-headless": "ng test --watch=false --code-coverage --browsers=ChromeHeadless --reporters=progress,junit",
    "test-chrome": "ng test --browsers=Chrome",
    "test-firefox": "ng test --browsers=Firefox",
    "test:coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "test:generate-publish-coverage": "npm run test-single-headless && npm run test:coveralls",
    "analyze-bundle": "ng build --prod --stats-json && webpack-bundle-analyzer dist/ng-radio/stats.json",
    "format:check": "prettier --list-different \"src/**/*{.ts,.json,.scss}\"",
    "format:fix": "prettier --write \"src/**/*{.ts,.json,.scss}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.5",
    "@angular/cdk": "~10.1.0",
    "@angular/common": "~10.0.5",
    "@angular/compiler": "~10.0.5",
    "@angular/core": "~10.0.5",
    "@angular/forms": "~10.0.5",
    "@angular/material": "^10.1.0",
    "@angular/platform-browser": "~10.0.5",
    "@angular/platform-browser-dynamic": "~10.0.5",
    "@angular/router": "~10.0.5",
    "@microsoft/applicationinsights-web": "^2.5.6",
    "@ngrx/effects": "^10.1.2",
    "@ngrx/router-store": "^10.1.2",
    "@ngrx/store": "^10.1.2",
    "@ngrx/store-devtools": "^10.1.2",
    "core-js": "^3.6.5",
    "dayjs": "1.8.30",
    "is-falsy-or-whitespace": "0.0.15",
    "jasmine-theories": "0.1.3",
    "lodash-es": "^4.17.15",
    "mat-progress-buttons": "^9.1.1",
    "material-design-icons": "^3.0.1",
    "ngx-mat-select-search": "^3.0.1",
    "oidc-client": "^1.10.1",
    "primeicons": "^4.0.0",
    "primeng": "^11.4.2",
    "rxjs": "~6.6.0",
    "socket.io-client": "^2.3.0",
    "subsink": "^1.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "^10.0.4",
    "@angular/compiler-cli": "^10.0.5",
    "@angular/language-service": "~10.0.5",
    "@ngrx/schematics": "^10.1.2",
    "@types/jasmine": "~3.5.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.0.26",
    "@types/socket.io-client": "^1.4.33",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "puppeteer": "5.2.1",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "tslint-etc": "^1.13.0",
    "typescript": "3.9.7",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
