{
  "name": "ng-radio",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint:fix": "ng lint --fix ng-radio",
    "test-single-headless": "ng test --watch=false --code-coverage --browsers=ChromeHeadless --reporters=progress,junit",
    "test-chrome": "ng test --browsers=Chrome",
    "test-firefox": "ng test --browsers=Firefox",
    "test:coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "test:generate-publish-coverage": "npm run test-single-headless && npm run test:coveralls",
    "analyze-bundle": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/ng-radio/stats.json",
    "format:check": "prettier --list-different \"src/**/*{.ts,.json,.scss}\"",
    "format:fix": "prettier --write \"src/**/*{.ts,.json,.scss}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/material": "^13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@microsoft/applicationinsights-web": "^2.5.6",
    "@ngrx/effects": "^12.5.1",
    "@ngrx/router-store": "^12.5.1",
    "@ngrx/store": "^12.5.1",
    "@ngrx/store-devtools": "^12.5.1",
    "core-js": "^3.6.5",
    "dayjs": "1.8.30",
    "is-falsy-or-whitespace": "0.0.15",
    "lodash-es": "^4.17.15",
    "mat-progress-buttons": "^9.3.1",
    "material-design-icons": "^3.0.1",
    "ngx-mat-select-search": "^3.3.0",
    "oidc-client": "^1.10.1",
    "primeicons": "^4.0.0",
    "primeng": "^12.2.2",
    "rxjs": "~6.6.0",
    "socket.io-client": "^2.3.0",
    "subsink": "^1.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.1",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "^13.0.1",
    "@angular/compiler-cli": "^13.0.0",
    "@angular/language-service": "~13.0.0",
    "@ngrx/schematics": "^12.5.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.0.26",
    "@types/socket.io-client": "^1.4.33",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "codelyzer": "^6.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.26.0",
    "jasmine-core": "~3.6.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "puppeteer": "^11.0.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "tslint-etc": "^1.13.10",
    "typescript": "4.4.4",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}