{
  "extends": [ "tslint:recommended", "tslint-etc" ],
  "rulesDirectory": [
    "codelyzer"
  ],
  "rules": {
    "no-unused-declaration": true, // Use tslint-etc to check for unused imports
    "no-string-literal": false,
    "no-console": false,
    "only-arrow-functions": true,
    "arrow-return-shorthand": true
  }
}
